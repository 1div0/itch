{
  "application/javascript": {
    "passthrough": true
  },
  "text/typescript": {
    "target": "es2017",
    "lib": ["dom", "dom.iterable", "scripthost", "es6"],
    "module": "commonjs",
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "sourceMap": true,
    "inlineSourceMap": true,
    "jsx": "react",
    "noEmitOnError": true
  }
}
